{
  "name": "capstone_apiprototype",
  "version": "1.0.0",
  "description": "Project for QUT IT Capstone, 2019, produced by Bitwise Productions for the client Synengco",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "npm run build-server && npm run build-client",
    "serve": "nodemon server/dist/index.js",
    "serve-once": "node server/dist/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript-Server,Typescript-Client,Node\" -c \"cyan.bold,cyan.bold,green.bold\" \"npm run watch-server\" \"npm run watch-client\" \"npm run serve\"",
    "build-server": "tsc -p ./server",
    "watch-server": "tsc -w -p ./server",
    "build-client": "webpack --display-error-details",
    "watch-client": "webpack -w --display-error-details",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect server/dist/index.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript-Server,Typescript-Client,Node\" -c \"cyan.bold,cyan.bold,green.bold\" \"npm run watch-ts-server\" \"npm run watch-ts-client\" \"npm run serve-debug\"",
    "doc-compile": "jsdoc -r -c ./jsdoc.config.js -a all -d ./docs && npm run doc-show",
    "doc-show": "node jsdoc.run.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Technosasquach/Capstone_APIPrototype"
  },
